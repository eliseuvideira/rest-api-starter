  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@ev-fns/auth": "^1.0.1",
    "@ev-fns/dotenv": "^1.0.1",
    "@ev-fns/endpoint": "^1.0.0",
    "@ev-fns/errors": "^0.1.1",
    "@ev-fns/openapi": "^0.3.2",
    "@ev-fns/server": "^0.2.1",
    "@ev-fns/validation": "^0.2.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.8.4",
    "moment": "^2.29.4",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage && xdg-open ./coverage/lcov-report/index.html &>/dev/null",
    "dev": "ts-node-dev --watch '*.yml,*.ts,*.js,*.json' src/server.ts",
    "docker": "./scripts/docker",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint --max-warnings=0 'src/**/*'",
    "postbuild": "find build -iname \"*.js\" -exec yarn uglifyjs {} --rename --toplevel --comments '/^\\*/' --output {} \\;",
    "prebuild": "rm -rf build",
    "prepare": "husky install",
    "start": "node build/server.js",
    "test": "jest",
    "watch": "jest --watch"
  }
}
